{
    "name": "thenextweb/passgenerator",
    "description": "A Laravel package to create Apple Wallet (old Passbook) compatible tickets.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "php",
        "laravel",
        "Apple",
        "Passbook"
    ],
    "authors": [
        {
            "name": "The Next Web BV",
            "email": "support@thenextweb.com"
        }
    ],
    "require": {
        "ext-zip": "*",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "thecodingmachine/safe": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "nunomaduro/larastan": "^0.6",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "rector/rector": "^0.7",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Thenextweb\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thenextweb\\PassGeneratorServiceProvider"
            ],
            "aliases": {
                "PassGenerator": "Thenextweb\\Facades\\PassGenerator"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
